[{
        title: '@@auditrail.title@@',
        properties:[{
            name : 'id',
            label : '@@auditrail.id@@',
            type : 'textfield',
            required : 'True',
            regex_validation : '^[a-zA-Z0-9_]+$',
            validation_message : '@@form.textfield.invalidId@@'
        }, {
	        name : 'loadBinder',
	        label : '@@auditrail.loadBinder@@',
	        type : 'elementselect',
	        value : '%s',
	        options_ajax : '[CONTEXT_PATH]/web/property/json/getElements?classname=org.joget.apps.form.model.FormLoadMultiRowElementBinder',
	        url : '[CONTEXT_PATH]/web/property/json[APP_PATH]/getPropertyOptions'
	    }, {
				name : 'formDefId',
				label : '@@auditrail.formDefId@@',
				type : 'selectbox',
				required : 'true',
				options_ajax : '[CONTEXT_PATH]/web/json/console/app[APP_PATH]/forms/options'
		}, {
				name : 'fieldProcessId',
				label : '@@auditrail.fieldProcessId@@',
				type : 'textfield',
				required : 'true',
				description : '@@auditTrailFormStoreBinder.fieldProcessId.description@@'
		}, {
            name : 'columns',
            label : '@@auditrail.columns.title@@',
            type : 'grid',
            columns : [{
                key : 'columnId',
                label : '@@auditrail.columns.id@@'
            },
            {
                key : 'columnLabel',
                label : '@@auditrail.columns.label@@'
            }]
        },{
            name : 'enableSort',
            label : '@@auditrail.enableSort@@',
            type : 'checkbox',
            value : 'False',
            options : [{
                value : 'true',
                label : ''
            }]
        },{
            name : 'sortBy',
            label : '@@auditrail.sortBy@@',
            description: '@@auditrail.sortBy.desc@@',
            type : 'textfield'
        }]
}]